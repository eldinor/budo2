{
  "name": "shader-reload-cli",
  "version": "1.0.0",
  "description": "A simple GLSL development server for testing and expreimentation.",
  "main": "index.js",
  "bin": {
    "shader-reload-cli": "./bin/index.js"
  },
  "author": "Matt DesLauriers",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/shader-reload/cli ./src/example/regl",
    "start:bin": "node src/budolite/cmd.js src/example-web/app.js --serve=bundle.js --live -v --dir src/example-web -- -t [ babelify --presets [ @babel/preset-env ] ]",
    "start:servelive": "node example/live.js example/app.js:bundle.js --dir example -- -t [ babelify --presets [ es2015 ] ]",
    "start:pushstate": "node bin/cmd.js example/app.js:bundle.js -v --dir example --live --pushstate -- -t [ babelify --presets [ es2015 ] ]",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/shader-reload-cli.git"
  },
  "dependencies": {
    "@babel/core": "7.16.5",
    "babelify": "10.0.0",
    "@babel/preset-env": "7.16.5",
    "sucrase": "3.20.3",
    "browser-resolve": "2.0.0",
    "is-core-module": "2.8.0",
    "acorn": "8.6.0",
    "serve-static": "^1.14.2",
    "glslify": "^7.1.1",
    "jsesc": "^3.0.2",
    "pumpify": "^2.0.1",
    "static-module": "^3.0.4",
    "through": "^2.3.8",
    "bole": "^4.0.0",
    "browserify": "^17.0.0",
    "chokidar": "^3.5.2",
    "connect-pushstate": "^1.1.0",
    "escape-html": "^1.0.3",
    "garnish": "^5.2.0",
    "get-ports": "^1.0.3",
    "inject-lr-script": "^2.2.0",
    "micromatch": "^4.0.4",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.2",
    "once": "^1.4.0",
    "open": "8.4.0",
    "lodash": "4.17.21",
    "reload-css": "^1.0.2",
    "resolve": "^1.20.0",
    "simple-html-index": "^1.5.0",
    "stacked": "^1.1.1",
    "stdout-stream": "^1.4.1",
    "subarg": "^1.0.0",
    "kolorist": "1.5.0",
    "url-trim": "^1.0.0",
    "watchify-middleware": "^1.8.2",
    "ws": "^8.3.0",
    "xtend": "^4.0.2",
    "qs": "6.10.2"
  },
  "devDependencies": {
    "budo": "11.6.4",
    "canvas-loop": "1.0.7",
    "2d-context": "1.3.0",
    "glslify": "^7.1.1",
    "primitive-plane": "^2.0.1",
    "glsl-noise": "0.0.0",
    "perspective-camera": "^2.0.1",
    "primitive-icosphere": "^1.0.2",
    "regl": "^2.1.0",
    "three": "npm:three@0.90.0"
  }
}
